{
    "name": "laminas-api-tools/api-tools-doctrine-querybuilder",
    "description": "QueryBuilder filters for Doctrine",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "api-tools",
        "api",
        "doctrine",
        "filter",
        "module"
    ],
    "homepage": "https://api-tools.getlaminas.org",
    "support": {
        "docs": "https://api-tools.getlaminas.org/documentation",
        "issues": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder/issues",
        "source": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder",
        "rss": "https://github.com/laminas-api-tools/api-tools-doctrine-querybuilder/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.3.23",
        "doctrine/doctrine-module": "~0.8@dev",
        "laminas-api-tools/api-tools-api-problem": "~1.0",
        "laminas/laminas-modulemanager": "~2.3",
        "laminas/laminas-mvc": "~2.3",
        "laminas/laminas-servicemanager": "~2.3",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "require-dev": {
        "doctrine/doctrine-mongo-odm-module": "0.*@dev",
        "doctrine/doctrine-orm-module": "0.*@dev",
        "laminas-api-tools/api-tools-provider": "~1.0",
        "laminas/laminas": "~2.4",
        "phpunit/phpunit": "~4.1"
    },
    "suggest": {
        "doctrine/doctrine-mongo-odm-module": "v0.*, if you wish to use the Doctrine ODM",
        "doctrine/doctrine-orm-module": "v0.*, if you wish to use the Doctrine ORM",
        "ext/mongo": "Mongo extension, if using ODM"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\Doctrine\\QueryBuilder\\": "src/"
        },
        "classmap": [
            "Module.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\ApiTools\\Doctrine\\QueryBuilder\\": "test/"
        }
    },
    "replace": {
        "zfcampus/zf-doctrine-querybuilder": "self.version"
    }
}
